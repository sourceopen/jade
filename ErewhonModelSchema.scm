itemMatchesSearch
{
itemMatchesSearch( pItem : Item ) : Boolean protected;
// --------------------------------------------------------------------------------
// Method:		itemMatchesSearch
//
// Purpose:		Checks whether a given Item matches a search
//
// Parameters:	pItem : The Item to check
//
// Returns:		A boolean representing whether an Item matches the search
// --------------------------------------------------------------------------------

begin
	// Check if closed tenders should be included
	if pItem.isKindOf( TenderItem ) 
	and pItem.TenderItem.isClosed() 
	and not self.includeClosedTenders then
		return false;
	endif;

	// Check if cart items should be included
	if not self.includeItemsInCart
	and app.getShoppingCartTA.contains( pItem ) then
		return false;
	endif;

	// Check if sold items should be included
	if not self.includeSoldItems
	and pItem.mySale <> null then
		return false;
	endif;
	
	// Check if item matches the agent
	if self.myAgent <> null 						
	and pItem.myAgent <> self.myAgent then			
		return false;								
	endif;
	
	// Check if item matches the client
	if self.myClient <> null 								
	and pItem.mySale = null then							
		return false;										
	elseif self.myClient <> null and pItem.mySale <> null then
		if pItem.mySale.myClient <> self.myClient then		
			return false;									
		endif;
	endif;
	
	// Check if item matches the country
	if self.myCountry <> null 
	and pItem.myRegion.myCountry <> self.myCountry then
		return false;
	endif;
	
	// Check if item matches the region
	if self.myRegion <> null 
	and pItem.myRegion <> self.myRegion then
		return false;
	endif;
	
	// Check if item matches the item category
	if self.myItemCategory <> null 
	and pItem.myItemCategory <> self.myItemCategory then
		return false;
	endif;
	
	// Check if item matches retail item
	if self.onlyRetailItems
	and pItem.isKindOf( TenderItem ) then
		return false;
	endif;
	
	// Check if item matches tender item
	if self.onlyTenderItems
	and pItem.isKindOf( RetailItem ) then
		return false;
	endif;
	
	// Check Low Price
	if self.priceLow <> null
	and pItem.price < self.priceLow then
		return false;
	endif;
	
	// Check High Price
	if self.priceHigh <> null 
	and pItem.price > self.priceHigh then
		return false;
	endif;
	
	return true;
end;
}
