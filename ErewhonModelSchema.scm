replaceString
{
replaceString(os:String;ns:String):Integer updating;

vars
	ix	:	Integer;
	iy	:	Integer;
	st	:	String;
	ct	:	Integer;
begin
	if self.length < os.length then
		return 0;
	endif;
	iy := 1;
	ix := 1;
	while ix <= self.length-os.length + 1 do
		ix := self.pos(os,ix);
		if ix = 0 then
			break;
		endif;
		st := st & self[iy:ix-iy] & ns;
		ct := ct + 1;
		iy := ix + os.length;
		ix := iy;
	endwhile;
	if iy <= self.length then
		st := st & self[iy:end];
	endif;
	self := st;
	return ct;
end;
}
