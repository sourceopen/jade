addItem
{
addItem( pItem : Item; pOffer : Decimal ) : Boolean updating;
// --------------------------------------------------------------------------------
// Method:		addItem
//
// Purpose:		Creates a CartItem proxy and adds it to the allCartItems collection.
//
// Parameters:	pItem 			The Item to add
//				pTenderOffer	The amount tendered (if applicable)
//
// Returns:		True if the item was added to the cart, otherwise returns False
// --------------------------------------------------------------------------------

vars
	cartItem : CartItem;
	retailSaleTA : RetailSaleTA;
	tenderTA : TenderTA;
	
begin
	// Clear any previous errors
	self.clearErrors();

	// confirm an Item has been provided
	if pItem = null then
		self.addError( $ItemRequired, null );
		return false;
	endif;
	
	// Listen for Update and Delete events for the item
	self.beginNotification( pItem, Object_Delete_Event, Response_Continuous, null );
	self.beginNotification( pItem, Object_Update_Event, Response_Continuous, null );
		
	// Create the CartItem proxy
	create cartItem transient;
	cartItem.code := pItem.getCode();
	cartItem.myItem := pItem;
	cartItem.offer := pOffer;
	
	if pItem.isKindOf( TenderItem ) then
		cartItem.transaction := $TenderItemType;
		
		// Check that tender price is valid
		if pOffer < pItem.price then
			self.addError( $TenderOfferLessThanReserve, Focus_Tender );
			return false;
		endif;
		
		create tenderTA transient;
		tenderTA.myTenderItem := pItem.TenderItem;
		tenderTA.myClient := app.getClient();
		tenderTA.offer := pOffer;
		tenderTA.dateTendered := Utilities@getServerTimeStamp();
		
		cartItem.offer := pOffer;
		cartItem.myTenderTA := tenderTA;
		
	else
		cartItem.transaction := $RetailItemType;
	
		create retailSaleTA transient;
		retailSaleTA.salePrice := pItem.price;
		retailSaleTA.myItem := pItem;
		retailSaleTA.myClient := app.getClient();
		
		cartItem.myRetailSaleTA := retailSaleTA;
	endif;

	// Assign cart item to the shopping cart.
	// Because of the automatic inverse, the cart item will available in the allCartItems collection
	if self.hasNoErrors() then
		cartItem.myShoppingCartTA := self;
	endif;
	
	return self.hasNoErrors();
end;
}
