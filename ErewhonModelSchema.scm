shortDate
{
shortDate() : String;

/*	
	We can't use the Jade shortDate method as this results in loss of any leading
	zeros for most locales.  This method does a locale aware Date.format with a 
	pattern that ensures the correct order and separators are used for the date,
	while forcing the date to always have leading zeros.
*/

vars
	dateSeparator		: String;
	shortFormatOrder	: Integer;
	shortDate			: String;
	dayString			: String;
	monthString			: String;
	yearString			: String;
	
begin
	// We don't want *invalid* to appear in any textboxes, particularly if they have a mask that prevents this
	if not self.isValid or self = null then
		return null;
	endif;
	
	dateSeparator	:= app.currentLocaleInfo.dateInfo.separator;
	shortFormatOrder	:= app.currentLocaleInfo.dateInfo.shortFormatOrder;
	
	if self.year > 1601 then
		if		shortFormatOrder = 0 then
			shortDate	:= self.format( 'MM' & dateSeparator & 'dd' & dateSeparator & 'yyyy' );
		elseif	shortFormatOrder = 1 then
			shortDate	:= self.format( 'dd' & dateSeparator & 'MM' & dateSeparator & 'yyyy' );
		elseif	shortFormatOrder = 2 then
			shortDate	:= self.format( 'yyyy' & dateSeparator & 'MM' & dateSeparator & 'dd' );
		else
			// shortFormatOrder is supposed to be 0, 1 or 2.  If it is not, we'll assume dd/MM/yyyy order...
			shortDate	:= self.format( 'dd' & dateSeparator & 'MM' & dateSeparator & 'yyyy' );
		endif;
	else
		// Years befor 1601 don't format properly in Windows, so assume 2 chars for day & month and 4 chars for year
		dayString	:= self.day.padLeadingWith( '0', 2 );
		monthString	:= self.month.padLeadingWith( '0', 2 );
		yearString	:= self.year.padLeadingWith( '0', 4 );
		if		shortFormatOrder = 0 then
			shortDate	:= dayString & dateSeparator & monthString & dateSeparator & yearString;
		elseif	shortFormatOrder = 1 then
			shortDate	:= dayString & dateSeparator & monthString & dateSeparator & yearString;
		elseif	shortFormatOrder = 2 then
			shortDate	:= yearString & dateSeparator & monthString & dateSeparator & dayString;
		else
			// shortFormatOrder is supposed to be 0, 1 or 2.  If it is not, we'll assume dd/MM/yyyy order...
			shortDate	:= dayString & dateSeparator & monthString & dateSeparator & yearString;
		endif;
	endif;
	
	return shortDate;
end;
}
