setColorFromRGB
{
setColorFromRGB( pRed : Integer; pGreen : Integer; pBlue : Integer ) updating;
// --------------------------------------------------------------------------------
// Method:		setColorFromRGB
//
// Purpose:		Converts RGB values into an integer value
//
// Parameters:	pRed	A value between 0-255 (#FF) representing the red intensity
//				pGreen	A value between 0-255 (#FF) representing the green intensity
//				pBlue	A value between 0-255 (#FF)representing the blue intensity
//
// Returns:		An integer between 0 - 16,777,215 (#FFFFFF) representing an RGB color
// --------------------------------------------------------------------------------

vars
	red 	: Integer;
	green 	: Integer;
	blue 	: Integer;
	
begin
	// Grab the colour element but restrict to maximum of 255
	red := pRed.min( 255 );
	green := pGreen.min( 255 );
	blue := pBlue.min( 255 );
	
	// Convert the RGB into an integer
	self := red + green * 256 + blue * 256 * 256;
end;
}
