getNextCodeNumber
{
getNextCodeNumber() : Integer updating;
// --------------------------------------------------------------------------------
// Method:		getNextCodeNumber
//
// Purpose:		Generates the next highest code number for an item
//
// Parameters:	N/A
//
// Returns:		An Integer with the current highest item number plus 1 
// --------------------------------------------------------------------------------


constants
	StartingNumber : Integer = 1;
	Increment : Integer = 1;
	MaxAttempts : Integer = 3;
	
vars
	lockObtained : Boolean;
	attempts : Integer;
	
begin
	attempts := 0;
	
	// Attempt to lock the allItems collection to avoid any additional items being added to the colleciton
	while not tryLock( app.myCompany.allItems, Exclusive_Lock, Session_Duration, LockTimeout_Process_Defined ) do
		attempts += 1;
		
		if attempts >= MaxAttempts then
			app.raiseErewhonException( Ex_500001_Lock_not_obtained );	
		endif;
		
		// Wait for 100ms before attempting to lock again so other processes 
		// have an opportunity to complete their work and release their locks
		process.sleep( 100 );
	endwhile;

	lockObtained := true;
	
	if app.myCompany.allItems.isEmpty() then
		return StartingNumber;
	endif;
	
	return app.myCompany.allItems.last().codeNumber + Increment;

epilog
	if lockObtained then
		unlock( app.myCompany.allItems );
	endif;
end;
}
